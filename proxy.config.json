{
  "/api/web-socket*": {
    "target": "http://localhost:9000",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "timeout": 600000,
    "ws": true,
    "pathRewrite": {
      "^/api": "/"
    }
  },

  "/api/*": {
    "target": "http://localhost:9000",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "timeout": 600000,
    "pathRewrite": {
      "^/api": "/"
    }
  }
}